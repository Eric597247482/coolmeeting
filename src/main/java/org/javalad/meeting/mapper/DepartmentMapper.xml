<!DOCTYPE mapper
        PUBLIC "-//mybatios.org//DTD Mapper 3.0//EN"
        "http://mybatis/org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.javalad.meeting.mapper.DepartmentMapper">
    <select id="getDepById" resultType="org.javalad.meeting.model.Department">
        select * from department where departmentid=#{id};
    </select>
</mapper>